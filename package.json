{
  "name": "ip-graph-extension",
  "version": "1.0.0",
  "description": "Browser extension for visualizing IP relationships and ownership on Story Protocol",
  "main": "dist/background.js",
  "scripts": {
    "build": "webpack --mode=production && npm run copy-files",
    "dev": "webpack --mode=development --watch",
    "copy-files": "cp src/popup/popup.html dist/ && cp src/popup/popup.css dist/ && cp manifest.json dist/ && cp -r src/assets dist/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "viem": "^2.0.0",
    "@walletconnect/sign-client": "^2.10.0",
    "@walletconnect/utils": "^2.10.0",
    "@walletconnect/types": "^2.10.0"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "typescript": "^5.1.6",
    "ts-loader": "^9.4.4",
    "@types/d3": "^7.4.0",
    "@types/chrome": "^0.0.246"
  },
  "keywords": ["story-protocol", "ip", "blockchain", "browser-extension"],
  "author": "Your Name",
  "license": "MIT"
}