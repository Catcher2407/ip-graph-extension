{
  "name": "ip-graph-extension",
  "version": "1.0.0",
  "description": "Browser extension for visualizing IP relationships and ownership on Story Protocol",
  "main": "dist/background.js",
  "scripts": {
    "build": "webpack --mode=production && npm run copy-files",
    "dev": "webpack --mode=development --watch",
    "copy-files": "cp src/popup/popup.html dist/ && cp src/popup/popup.css dist/ && cp manifest.json dist/ && cp -r src/assets dist/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@reown/appkit": "^1.7.17",
    "@reown/appkit-adapter-wagmi": "^1.7.17",
    "@tanstack/react-query": "^5.84.1",
    "@types/chrome": "^0.1.1",
    "@web3modal/wagmi": "^5.0.0",
    "d3": "^7.9.0",
    "viem": "^2.33.2",
    "wagmi": "^2.16.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "story-protocol",
    "ip",
    "blockchain",
    "browser-extension"
  ],
  "author": "Your Name",
  "license": "MIT"
}
